{
  "env": {
    "es6": true,
    "amd": true,
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-trailing-spaces": 2,
    "indent": [ "error", 2],
    "camelcase": 2
  }
}